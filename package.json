{
  "name": "bot-template",
  "version": "1.0.2",
  "description": "A basic Discord bot template using @ssMMiles/interactions.ts",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "format": "prettier --write src",
    "lint": "eslint src/**/*.ts",
    "build": "yarn format & yarn lint && tsc -p tsconfig.json",
    "build:docker": "docker build -t bot-template .",
    "start": "node dist/index.js",
    "dev": "yarn build && yarn start"
  },
  "author": "ssMMiles",
  "license": "MIT",
  "dependencies": {
    "@discord-interactions/builders": "^0.2.6",
    "@discord-interactions/core": "^0.2.6",
    "@discord-interactions/verify-node": "^0.2.6",
    "discord-api-types": "^0.32.1",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.4",
    "fastify-raw-body": "^3.2.0",
    "redis": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.18.3",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "prettier": "^2.5.1",
    "typescript": "^4.7.4"
  }
}
